<h1>Listing site_setups</h1>

<table>
  <thead>
    <tr>
      <th>Youtube</th>
      <th>Facebook</th>
      <th>Video stream</th>
      <th>Twitter</th>
      <th>Life pulse</th>
      <th>Leaders blog</th>
      <th>Store</th>
      <th>Video</th>
      <th>Articles</th>
      <th>Shutdown</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @site_setups.each do |site_setup| %>
      <tr>
        <td><%= site_setup.youtube %></td>
        <td><%= site_setup.facebook %></td>
        <td><%= site_setup.video_stream %></td>
        <td><%= site_setup.twitter %></td>
        <td><%= site_setup.life_pulse %></td>
        <td><%= site_setup.leaders_blog %></td>
        <td><%= site_setup.store %></td>
        <td><%= site_setup.video %></td>
        <td><%= site_setup.articles %></td>
        <td><%= site_setup.shutdown %></td>
        <td><%= link_to 'Show', site_setup %></td>
        <td><%= link_to 'Edit', edit_site_setup_path(site_setup) %></td>
        <td><%= link_to 'Destroy', site_setup, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Site setup', new_site_setup_path %>
