<h1>Listing events_pictures</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Event image</th>
      <th>Linked event</th>
      <th>Description</th>
      <th>Event</th>
      <th>Eventimage</th>
      <th>Image processed</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events_pictures.each do |events_picture| %>
      <tr>
        <td><%= events_picture.title %></td>
        <td><%= events_picture.event_image %></td>
        <td><%= events_picture.linked_event %></td>
        <td><%= events_picture.description %></td>
        <td><%= events_picture.event_id %></td>
        <td><%= events_picture.eventimage %></td>
        <td><%= events_picture.image_processed %></td>
        <td><%= link_to 'Show', events_picture %></td>
        <td><%= link_to 'Edit', edit_events_picture_path(events_picture) %></td>
        <td><%= link_to 'Destroy', events_picture, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Events picture', new_events_picture_path %>
