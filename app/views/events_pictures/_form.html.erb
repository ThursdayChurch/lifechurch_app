<%= simple_form_for(@events_picture) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :event_image %>
    <%= f.input :linked_event %>
    <%= f.input :description %>
    <%= f.input :event_id %>
    <%= f.input :eventimage %>
    <%= f.input :image_processed %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
