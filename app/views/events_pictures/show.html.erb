<%- model_class = EventsPicture -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:title) %>:</strong></dt>
  <dd><%= @events_picture.title %></dd>
  <dt><strong><%= model_class.human_attribute_name(:event_image) %>:</strong></dt>
  <dd><%= @events_picture.event_image %></dd>
  <dt><strong><%= model_class.human_attribute_name(:linked_event) %>:</strong></dt>
  <dd><%= @events_picture.linked_event %></dd>
  <dt><strong><%= model_class.human_attribute_name(:description) %>:</strong></dt>
  <dd><%= @events_picture.description %></dd>
  <dt><strong><%= model_class.human_attribute_name(:event_id) %>:</strong></dt>
  <dd><%= @events_picture.event_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:eventimage) %>:</strong></dt>
  <dd><%= @events_picture.eventimage %></dd>
  <dt><strong><%= model_class.human_attribute_name(:image_processed) %>:</strong></dt>
  <dd><%= @events_picture.image_processed %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              events_pictures_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_events_picture_path(@events_picture), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              events_picture_path(@events_picture),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
