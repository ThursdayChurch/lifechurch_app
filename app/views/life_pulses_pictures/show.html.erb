<%- model_class = LifePulsesPicture -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:life_pulse_id) %>:</strong></dt>
  <dd><%= @life_pulses_picture.life_pulse_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:life_pulse_image) %>:</strong></dt>
  <dd><%= @life_pulses_picture.life_pulse_image %></dd>
  <dt><strong><%= model_class.human_attribute_name(:title) %>:</strong></dt>
  <dd><%= @life_pulses_picture.title %></dd>
  <dt><strong><%= model_class.human_attribute_name(:summary) %>:</strong></dt>
  <dd><%= @life_pulses_picture.summary %></dd>
  <dt><strong><%= model_class.human_attribute_name(:image_processed) %>:</strong></dt>
  <dd><%= @life_pulses_picture.image_processed %></dd>
  <dt><strong><%= model_class.human_attribute_name(:main_page) %>:</strong></dt>
  <dd><%= @life_pulses_picture.main_page %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              life_pulses_pictures_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_life_pulses_picture_path(@life_pulses_picture), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              life_pulses_picture_path(@life_pulses_picture),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
