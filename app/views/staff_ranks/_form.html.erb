<%= simple_form_for @staff_rank, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :ranking %>
  <%= error_span(@staff_rank[:ranking]) %>
  <%= f.input :category %>
  <%= error_span(@staff_rank[:category]) %>

  <%= f.button :submit, :class => 'btn-primary' %>
  <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                staff_ranks_path, :class => 'btn btn-default' %>
<% end %>
