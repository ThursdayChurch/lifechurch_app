<%- model_class = ChurchStaffsPicture -%>

 
<div class="page-header">
  <h2>	<% if @churchs_staff_picture.church_staff != nil %>

	    <%= @churchs_staff_picture.church_staff.full_name %>

	  <% else %>
	    <em>Not setup to Staff</em>
	  <% end %>  - Picture </h2>
</div>

<dl class="dl-horizontal">
  <dt><strong>Name:</strong></dt>
  <dd><%= @churchs_staff_picture.first_name %>
	
		 <% if @churchs_staff_picture.church_staff != nil %>

		    <%= @churchs_staff_picture.church_staff.full_name %>

		  <% else %>
		    <em>Not setup to Staff</em>
		  <% end %>
	</dd>
	
	<br>
	<br>
 
  <dt><strong>Picture:</strong></dt>
  <dd>	<% if @churchs_staff_picture.image_processed? %>
	    <%= image_tag(@churchs_staff_picture.staff_image_url(:thumb), :class =>"img-rounded" ) %>

	  <% else %>
	    <em>Processing...</em>
	  <% end %>
	
	
	</dd>
 
</dl>

<div class="form-actions">
 
			  
			  <%= link_to t('.back', :default => t("helpers.links.back")),
			                church_staffs_pictures_path, :class => 'btn btn-default'  %>
		 
			  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
			                church_staffs_picture_path(@church_staffs_picture),
			                :method => 'delete',
			                :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
			                :class => 'btn btn-danger' %>
			  
</div>

 