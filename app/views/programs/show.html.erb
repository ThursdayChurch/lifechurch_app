<%- model_class = Program -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:program_name) %>:</strong></dt>
  <dd><%= @program.program_name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:contact_email) %>:</strong></dt>
  <dd><%= @program.contact_email %></dd>
  <dt><strong><%= model_class.human_attribute_name(:location) %>:</strong></dt>
  <dd><%= @program.location %></dd>
  <dt><strong><%= model_class.human_attribute_name(:contact_person) %>:</strong></dt>
  <dd><%= @program.contact_person %></dd>
  <dt><strong><%= model_class.human_attribute_name(:description) %>:</strong></dt>
  <dd><%= @program.description %></dd>
  <dt><strong><%= model_class.human_attribute_name(:remove_program_date) %>:</strong></dt>
  <dd><%= @program.remove_program_date %></dd>
  <dt><strong><%= model_class.human_attribute_name(:display_main_page) %>:</strong></dt>
  <dd><%= @program.display_main_page %></dd>
  <dt><strong><%= model_class.human_attribute_name(:event_id) %>:</strong></dt>
  <dd><%= @program.event_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:advertisement_id) %>:</strong></dt>
  <dd><%= @program.advertisement_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:slug) %>:</strong></dt>
  <dd><%= @program.slug %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              advertisements_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_program_path(@program), :class => 'btn btn-default' %>
 