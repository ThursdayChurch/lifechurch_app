<%= simple_form_for(@ministries_picture) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :ministry_image %>
    <%= f.input :image_processed %>
    <%= f.input :ministries_id %>
    <%= f.input :ministry_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
