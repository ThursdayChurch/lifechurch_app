<%= simple_form_for(@speaker) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :first_name %>
    <%= f.input :last_name %>
    <%= f.input :from_church %>
    <%= f.input :description %>
    <%= f.input :church_staff_id %>
    <%= f.input :from_church_url %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
