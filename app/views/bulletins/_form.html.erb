<%= simple_form_for(@bulletin) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :display_till %>
    <%= f.input :display_start %>
    <%= f.input :advert_main_page %>
    <%= f.input :bulletin_image %>
    <%= f.input :url %>
    <%= f.input :members_only %>
    <%= f.input :image_processed %>
    <%= f.input :slug %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
