<div class="col-md-offset-1 col-md-9">
<br>
<%= simple_form_for @bulletin, :html => { :class => 'form-horizontal' } do |f| %>
 
    <%= f.input :display_start, as: :date, :label=>"Start to Display:", :placeholder=>"First Date to Display",  :hint => "First Date to Display",  start_year: Date.today.year, 
        end_year: Date.today.year + 2, order: [ :month,:day, :year], :include_blank => false, :input_html => { :class => "enroll_input span2"}    %>
  <%= error_span(@bulletin[:display_start]) %>
  
  
    <%= f.input :key, as: :hidden %>
	

  <%= f.button :submit, :class => 'btn-primary' %>
  <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                bulletins_path, :class => 'btn btn-default' %>
<% end %>
</div>