<%- model_class = MinistryChildrenPicture -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:ministrychild_image) %>:</strong></dt>
  <dd><%= @ministry_children_picture.ministrychild_image %></dd>
  <dt><strong><%= model_class.human_attribute_name(:image_processed) %>:</strong></dt>
  <dd><%= @ministry_children_picture.image_processed %></dd>
  <dt><strong><%= model_class.human_attribute_name(:ministrychild_id) %>:</strong></dt>
  <dd><%= @ministry_children_picture.ministrychild_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:ministry_id) %>:</strong></dt>
  <dd><%= @ministry_children_picture.ministry_id %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              ministry_children_pictures_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_ministry_children_picture_path(@ministry_children_picture), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              ministry_children_picture_path(@ministry_children_picture),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
