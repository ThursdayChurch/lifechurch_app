<%= simple_form_for(@schedule) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :end_month %>
    <%= f.input :start_month %>
    <%= f.input :schedule_image %>
    <%= f.input :image_processed %>
    <%= f.input :url %>
    <%= f.input :slug %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
